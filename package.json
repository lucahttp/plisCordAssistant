{
  "name": "pliscord-assistant",
  "version": "1.0.0",
  "type": "module",
  "description": "On-device voice assistant for Discord and driving",
  "main": "discord-bot/index.js",
  "scripts": {
    "discord": "node discord-bot/index.js",
    "drive": "node driving-assistant/index.js",
